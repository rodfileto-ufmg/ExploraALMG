    "---\n",
    "title: \"Explorando dados abertos da ALMG\"\n",
    "date-modified: last-modified\n",
    "repo-url: https://github.com/rodfileto-ufmg/ExploraALMG\n",
    "engine: jupyter\n",
    "lang: pt\n",
    "author: \"Rodrigo Fileto\"\n",
